{
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": true,
		"source.organizeImports": true
	},
	"files.exclude": {
		"**/node_modules": true
	},
	"search.exclude": {
		"**/node_modules": true,
		"**/bower_components": true,
		"**/_build": true,
		"**/deps": true,
		"**/dist": true,
		"**/.cache": true,
		"typescript/**": true
	},
	"files.watcherExclude": {
		"**/.git/objects/**": true,
		"**/.git/subtree-cache/**": true,
		"**/node_modules/**": true,
		"**/bower_components": true,
		"**/_build": true,
		"**/deps": true
	},
	"eslint.validate": [
		"javascript",
		"javascriptreact",
		"typescript",
		"typescriptreact"
	],
	"editor.tabSize": 2,
	"thunder-client.saveToWorkspace": true,
	"thunder-client.workspaceRelativePath": "./",
	"thunder-client.customLocation": "",
}